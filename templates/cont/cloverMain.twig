{% extends "/layouts/plain.twig" %}

{% block title %}ページ情報{% endblock %}
{% block head %}
<link rel="stylesheet" href="{{ path_client_resources }}cloverMain/cloverMain.css" />
{% endblock %}
{% block content %}

<div id="cont-app"></div>

{% endblock %}
{% block foot %}
<script>
window.px2config = {
    "name": "{{ px2config.name | e("js") }}",
    "path_controot": "{{ px2config.path_controot | e("js") }}",
    "default_directory_index": "{{ px2config.directory_index[0] | e("js") }}",
};
window.csrf_token = "{{ csrf_token | e("js") }}";
</script>
<script src="{{ path_client_resources }}cloverMain/cloverMain.js"></script>
{% endblock %}
